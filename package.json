{
  "name": "snapp-site",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && react-snap",
    "lint": "vue-cli-service lint",
    "now-build": "vue-cli-service build",
    "now-static": "vue-cli-service build && react-snap && cp config/now.json dist && cd dist && now --prod",
    "deploy": "now --prod"
  },
  "dependencies": {
    "crypto-js": "^3.1.9-1",
    "js-cookie": "^2.2.0",
    "node-sass": "^7.0.0",
    "react-snap": "^1.23.0",
    "sass-loader": "^7.1.0",
    "simple-analytics-vue": "^1.1.0",
    "vue": "^2.5.17",
    "vue-croppa": "^1.3.8",
    "vue-i18n": "^8.2.1",
    "vue-meta": "^1.5.8",
    "vue-router": "^3.0.2",
    "vue-scrollto": "^2.13.0",
    "vuex": "^3.0.1",
    "vuex-persist": "^1.8.0",
    "vuex-persistedstate": "^2.5.4"
  },
  "devDependencies": {
    "@kazupon/vue-i18n-loader": "^0.3.0",
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-eslint": "^3.2.1",
    "@vue/cli-service": "^3.2.0",
    "eslint-plugin-babel": "^5.2.1",
    "eslint-plugin-html": "^5.0.0",
    "swagger-client": "^3.8.22",
    "vue-template-compiler": "^2.5.17",
    "vuex-c3s": "^1.1.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "reactSnap": {
    "source": "dist",
    "minifyHtml": {
      "collapseWhitespace": false,
      "removeComments": false
    },
    "puppeteerArgs": [
      "--disable-setuid-sandbox",
      "--no-sandbox"
    ],
    "concurrency": 1,
    "inlineCss": false,
    "puppeteerExecutablePath": "/Applications/Google Chrome.app/Contents/MacOS/Google Chrome"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
